{
  "_tibanna": {
    "env": "fourfront-webdev",
    "run_type": "encode-chipseq"
  },
  "args": {
    "app_name": "encode-chipseq",
    "input_parameters": { 
        "chip.pipeline_type" : "tf",
        "chip.paired_end" : false,
        "chip.choose_ctl.always_use_pooled_ctl" : true,
        "chip.qc_report.name" : "test",
        "chip.qc_report.desc" : "CTCF ChIP-seq on mouse Patski wild-type by the Christine Disteche lab",
        "chip.gensz" : "mm",
        "chip.bam2ta.regex_grep_v_ta" : "chr[MUE]|random|alt",
        "chip.fraglen": [300],
        "chip.bwa.cpu": 16,
        "chip.merge_fastq.cpu": 16,
        "chip.filter.cpu": 16,
        "chip.bam2ta.cpu": 16,
        "chip.xcor.cpu": 16
    },
    "wdl_child_filenames": [],
    "language": "wdl",
    "output_target": {
       "chip.report": "testms3_chipseq_report.html",
       "chip.qc_json": "testms3_chipseq_qc.json",
       "chip.idr_ppr.bfilt_idr_peak_bb": "testms3_idr_ppr_peak.bb"
    },
    "secondary_output_target": {},
    "wdl_main_filename": "chip.wdl",
    "secondary_files": {},
    "output_S3_bucket": "soos-4dn-bucket",
    "app_version": "dev",
    "wdl_directory_url": "https://raw.githubusercontent.com/4dn-dcic/chip-seq-pipeline2/master/",
    "input_files": {
      "chip.fastqs": {
        "bucket_name": "soos-4dn-bucket",
        "object_key": [[["4DNFIE6Y5WQM.h40M.fastq.gz"], ["4DNFIX27ZKYY.h40M.fastq.gz"]]]
      },
      "chip.ctl_fastqs": {
        "bucket_name": "soos-4dn-bucket",
        "object_key": [[["4DNFII5JIXFQ.h40M.fastq.gz"], ["4DNFIU4MKVH6.h40M.fastq.gz"]]]
      },
      "chip.bwa_idx_tar": {
        "bucket_name": "elasticbeanstalk-fourfront-webdev-files",
        "rename": "mm10_no_alt_analysis_set_ENCODE.fasta.tar",
        "object_key": "f4b63d31-65d8-437f-a76a-6bedbb52ae6f/4DNFIZ2PWCC2.bwaIndex.tar"
      },
      "chip.blacklist": {
        "bucket_name": "elasticbeanstalk-fourfront-webdev-files",
        "object_key": "a32747a3-8a9e-4a9e-a7a1-4db0e8b65925/4DNFIZ3FBPK8.bed.gz"
      },
      "chip.chrsz": {
        "bucket_name": "elasticbeanstalk-fourfront-webdev-files",
        "object_key": "be0a9819-d2ce-4422-be4b-234fb1677dd9/4DNFIBP173GC.chrom.sizes"
      }
    }
  },
  "config": { 
    "ebs_size": 50,
    "ebs_type": "gp2",
    "json_bucket": "4dn-aws-pipeline-run-json",
    "EBS_optimized": false,
    "ebs_iops": "",
    "shutdown_min": 120,
    "instance_type": "c4.4xlarge",   
    "password": "dragonfly",
    "log_bucket": "tibanna-output",
    "key_name": ""
  }
