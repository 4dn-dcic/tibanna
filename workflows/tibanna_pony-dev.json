{
  "Comment": "Start a workflow run on awsem, (later) track it and update our metadata to reflect whats going on",
  "StartAt": "StartRunAwsem",
  "States": {
        "StartRunAwsem": {
                "Type": "Task",
                "Resource": "arn:aws:lambda:us-east-1:643366669028:function:start_run_awswem_dev",
                "Next": "RunTaskAwsem"
        },
        "RunTaskAwsem": {
                "Type": "Task",
                "Resource": "arn:aws:lambda:us-east-1:643366669028:function:run_task_awsem_dev",
                "Next": "CheckTaskAwsem"
        },
		"CheckTaskAwsem": {
				"Type": "Task",
				"Resource": "arn:aws:lambda:us-east-1:643366669028:function:check_task_awsem_dev",
				"Retry": [
				  {
					"ErrorEquals": ["EC2StartingException"],
					"IntervalSeconds": 300,
					"MaxAttempts" : 4,
					"BackoffRate" : 1.0
				   },
				  {
					"ErrorEquals": ["StillRunningException"],
					"IntervalSeconds": 600,
					"MaxAttempts" : 10000,
					"BackoffRate" : 1.0
				   }
				  ],
				"Next": "UpdateFFMetaAwsem" 
		},
		"UpdateFFMetaAwsem": {
				"Type": "Task",
				"Resource":"arn:aws:lambda:us-east-1:643366669028:function:update_ffmeta_awsem_dev",
				"End": true 
		}
  }
}
